    "jsdoc": "^4.0.4",
    "turbo": "^2.2.0",            
    "typescript": "^5.4.0" 
  },
  "dependencies": {
    "@fortawesome/react-fontawesome": "^0.2.2",
    "luxon": "^3.5.0"
  }
